{
  "name": "riannegreiros",
  "version": "1.0.0",
  "description": "Rian Negreiros's GitHub Profile",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"node build/app.js\"",
    "start": "node build/app.js",
    "generate": "npm run build && npm start",
    "lint": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,ts,json}\"",
    "clean": "rm -rf build"
  },
  "dependencies": {
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^25.0.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}